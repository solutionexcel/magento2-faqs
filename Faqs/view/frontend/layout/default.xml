<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="header.links">
			<block class="Magento\Framework\View\Element\Html\Link\Current" name="faqs_top" ifconfig="solutionexcel_faqs/general/is_enabled">
				<arguments>
					<argument name="label" xsi:type="string">FAQs</argument>
					<argument name="path" xsi:type="string">faqs</argument>
				</arguments>
			</block>        
		</referenceBlock>
		<referenceBlock name="footer_links">
			<block class="Magento\Framework\View\Element\Html\Link\Current" name="faqs_footer" ifconfig="solutionexcel_faqs/general/is_enabled">
				<arguments>
					<argument name="label" xsi:type="string">FAQs</argument>
					<argument name="path" xsi:type="string">faqs</argument>
				</arguments>
			</block>  
		</referenceBlock>
    </body>
</page>

	